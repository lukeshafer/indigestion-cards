---
import Page from '@/layouts/Page.astro';
import { getAllSeries } from '@lil-indigestion-cards/core/card'

const series = await getAllSeries()
---

<Page title="Series">
	<h1>Series</h1>
	<ul>
		{
			series.map((series) => (
				<li>
					<a href={`/series/${series.seriesId}`}>{series.seriesName}</a>
				</li>
			))
		}
	</ul>
</Page>
