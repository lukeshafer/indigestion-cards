{
  "extends": "astro/tsconfigs/strict",
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ES2020",
    "lib": [
      "ES2021"
    ],
    "allowJs": true,
    "baseUrl": ".",
    //"noUnusedLocals": true,
    //"noUnusedParameters": true,
    "paths": {
      "@admin/*": [
        "src/*"
      ],
      "@core/lib/*": [
        "../core/src/lib/*"
      ],
      "@core/types": [
        "../core/src/types"
      ],
      "@core/constants": [
        "../core/src/constants"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
