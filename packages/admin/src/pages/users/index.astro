---
import Page from '@admin/layouts/Page.astro';
import { getAllPreorders } from '@core/lib/preorder';
import { PageHeader, PageTitle } from '@admin/components/text';
import UsersTable from '@admin/components/user/UsersTable';
import { getAllUsers } from '@core/lib/user';

const users = await getAllUsers()
const preorders = await getAllPreorders();
---

<Page title="Users">
	<PageHeader>
		<PageTitle>Users</PageTitle>
	</PageHeader>
	<div class="mx-auto max-w-2xl">
		<UsersTable {users} {preorders} client:load />
	</div>
</Page>
