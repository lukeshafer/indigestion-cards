---
import Page from '@/layouts/Page.astro';
import AdminUserForm from '@/components/entity-forms/AdminUserForm.astro';
import { api, routes } from '@/constants';
---

<Page
	title="Add admin user"
	breadcrumbs={[
		{ label: 'Home', href: '/' },
		{ label: 'Admins', href: '/admin-users' },
		{ label: 'Add Admin User' },
	]}>
	<h1 class="page-heading">Add admin user</h1>
	<AdminUserForm action={`${api.CREATE_ADMIN_USER}?redirect=${routes.ADMIN_USERS}`} />
</Page>
