---
import PageHeading from '@/components/text/PageHeading.astro';
import Page from '@/layouts/Page.astro';
import { getAllPackTypes } from '@lil-indigestion-cards/core/card';
import GivePackForm from '@/components/pack/GivePackForm';

const packTypes = await getAllPackTypes();
---

<Page title="Give Pack" breadcrumbs={[{ label: 'Give Pack' }]}>
	<PageHeading>Give Pack</PageHeading>
	<GivePackForm {packTypes} client:load />
</Page>
