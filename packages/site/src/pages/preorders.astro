---
import Page from '@/layouts/Page.astro';
import { getAllPreorders } from '@lib/preorder';

const preorders = await getAllPreorders();
---

<Page title="Preorders" breadcrumbs={[{ label: 'Preorders' }]}>
	<ul>
		{
			preorders.map((preorder) => (
				<li>
					{preorder.username},{' '}
					{preorder.createdAt ? new Date(preorder.createdAt).toLocaleString() : ''}
				</li>
			))
		}
	</ul>
</Page>
